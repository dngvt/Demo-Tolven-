<?xml version="1.0" encoding="UTF-8"?>
<plugin id="org.tolven.deploy.observations" version="2.1.8" class="org.tolven.deploy.observations.LoaderObservations" vendor="Tolven" xmlns="urn:tolven-org:tpf:1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:tolven-org:tpf:1.0 http://tolven.org/xsd/tpf.xsd">
	<requires>
		<import plugin-id="org.tolven.library.application" />
	</requires>
	<runtime>
		<library id="classes" path="tpf/classes/" type="code">
			<export prefix="*" />
		</library>
	</runtime>
	<extension-point id="devLib" extension-multiplicity="none">
		<parameter-def id="jar" default-value="devLib.jar" />
	</extension-point>
	
	<extension-point id="runtimeLib" extension-multiplicity="none">
		<parameter-def id="gson" default-value="gson/gson-2.2.4.jar" multiplicity="none-or-one" />
	</extension-point>
	<extension-point id="org.tolven.deploy.observations-runtimeLib" parent-plugin-id="org.tolven.deploy.observations" parent-point-id="runtimeLib" extension-multiplicity="none">
		<parameter-def id="gson" multiplicity="none-or-one" />
	</extension-point>
	
</plugin>
