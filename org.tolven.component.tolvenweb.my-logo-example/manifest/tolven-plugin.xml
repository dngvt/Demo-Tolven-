<?xml version="1.0" encoding="UTF-8"?>
<plugin id="org.tolven.component.tolvenweb.my-logo-example" version="2.1.0" vendor="Tolven" xmlns="urn:tolven-org:tpf:1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:tolven-org:tpf:1.0 http://tolven.org/xsd/tpf.xsd">
	<requires>
	<import plugin-id="org.tolven.component.tolvenweb" />
	</requires>
	<runtime>
	</runtime>
	<extension-point id="devLib" extension-multiplicity="none">
		<parameter-def id="jar" default-value="devLib.jar" />
	</extension-point>
	<extension-point extension-multiplicity="none" id="tolvenProperties">
		<parameter-def id="tolven.web.defaultLogo" default-value="#{pluginProperty['tolven.web.defaultLogo']}" />
	</extension-point>
	<extension id="imagesDirectory" plugin-id="org.tolven.component.tolvenweb" point-id="imagesDirectory">
     <parameter id="sourceDirectory" value="web/images" />
</extension>

</plugin>
