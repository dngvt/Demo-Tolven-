<?xml version="1.0" encoding="UTF-8"?>
<plugin id="org.tolven.gatekeeper.my-logo-example" version="2.1.2" vendor="Tolven" xmlns="urn:tolven-org:tpf:1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:tolven-org:tpf:1.0 http://tolven.org/xsd/tpf.xsd">
	<requires>
	<import plugin-id="org.tolven.component.war" />
	<import plugin-id="org.tolven.gatekeeper" />
	</requires>
	<runtime>
	</runtime>
	<extension-point id="devLib" extension-multiplicity="none">
		<parameter-def id="jar" default-value="devLib.jar" />
	</extension-point>
	<extension-point extension-multiplicity="none" id="tolvenProperties">
		<parameter-def id="gatekeeper.web.defaultLogo" default-value="#{pluginProperty['gatekeeper.web.defaultLogo']}" />
	</extension-point>
	<extension-point id="my-logo-example-imagesDirectory" extension-multiplicity="none">
		<parameter-def id="sourceDirectory" default-value="web/images" multiplicity="none-or-one" />
	</extension-point>
	
	<extension id="notice-loginDirectory-webDirectory-adaptor" plugin-id="org.tolven.gatekeeper" point-id="webDirectory-adaptor">
		<parameter id="source-plugin-id" value="org.tolven.gatekeeper.my-logo-example" />
		<parameter id="extension-point" value="my-logo-example-imagesDirectory" />
		<parameter id="targetWebDirectory" value="images" />
		<parameter id="parent-context-id" value="gatekeeperhtml" />
	</extension>
	
	

</plugin>
